{
  "name": "@naturalcycles/puppeteer-lib",
  "scripts": {
    "html2pdf": "tsn ./src/bin/html2pdf.ts",
    "html2pdf-debug": "tsn ./src/bin/html2pdf.ts './test/**/*.html' --verbose"
  },
  "dependencies": {
    "@naturalcycles/promise-lib": "^2.0.0",
    "file-url": "^3.0.0",
    "fs-extra": "^7.0.1",
    "globby": "^9.2.0",
    "puppeteer": "^1.14.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@naturalcycles/semantic-release": "^1.0.0",
    "@naturalcycles/shared-module": "^5.14.5",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.7",
    "@types/puppeteer": "^1.12.3",
    "jest": "^24.7.1",
    "jest-junit": "^6.3.0",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.4"
  },
  "files": [
    "dist",
    "src",
    "!src/test",
    "!src/**/*.test.ts",
    "!src/**/__snapshots__",
    "!src/**/__exclude"
  ],
  "bin": {
    "html2pdf": "dist/bin/html2pdf.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NaturalCycles/puppeteer-lib"
  },
  "engines": {
    "node": ">=10.13"
  },
  "version": "1.1.1",
  "description": "CLI to quickly produce PDFs from HTML files using Puppeteer",
  "author": "Natural Cycles Team",
  "license": "MIT"
}
